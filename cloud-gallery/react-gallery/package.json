{
  "name": "react-gallery",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "npm run -w helpers build",
    "build": "vite build",
    "start": "npx wrangler dev --local",
    "deploy": "BASE_URL=/_fragment/react-gallery/ npx wrangler publish",
    "lint": "eslint \"{pages,renderer,worker}/**/*.ts*\"",
    "check-format": "prettier --check --ignore-path=../.prettierignore .",
    "format": "prettier --write --ignore-path=../.prettierignore .",
    "typecheck": "tsc --incremental --noEmit"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@vitejs/plugin-react": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "prettier": "2.7.1",
    "vite": "^3.2.2",
    "vite-plugin-ssr": "^0.4.44",
    "wrangler": "^2.1.15"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@cloudflare/workers-types": "^3.18.0",
    "typescript": "^4.8.4"
  }
}
